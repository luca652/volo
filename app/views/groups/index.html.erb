<div class= "groups-container">
  <div style="width: 100%; height: 200px;"
    data-controller="map"
    data-map-markers-value="<%= @markers.to_json %>"
    data-map-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>">
  </div>
</div>

<div class= "filter-group-container">
 <%= simple_form_for @filter_form, url: groups_path, method: :get do |form| %>
  <%= form.input :childrens_age, collection: ['0-2', '3-5', '6-8', '9+'], include_blank: 'All', multiple: true %>
  <%= form.button :submit, 'Apply Filter' %>
  <%= @groups_all %>
<% end %>
</div>

